{
  "log_dir": "logs/",
  "skip_downloading_content": true,
  "video_extensions": [
    "mp4",
    "avi",
    "mkv"
  ],
  "banned_words": [
    "Episode", "episode"
  ],
  "failure_words": [
    "Anime", "anime"
  ],
  "target_dir": [
    {
      "path": "D:\\Anime",
      "organize_root_anime_in_folder": true,
      "minimum_anime_to_create_folder": 2,
      "reccursion": {
        "use_dir_name": true,
        "excluded_files": []
      }
    }
  ]
}
